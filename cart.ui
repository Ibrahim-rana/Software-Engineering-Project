<?xml version="1.0" encoding="UTF-8"?>
<ui version="4.0">
 <class>CartWindow</class>
 <widget class="QMainWindow" name="CartWindow">
  <property name="windowTitle">
   <string>Shopping Cart</string>
  </property>
  <widget class="QWidget" name="centralwidget">
   <layout class="QVBoxLayout" name="verticalLayout">
    
    <!-- GroupBox for Cart Items -->
    <item>
     <widget class="QGroupBox" name="cartGroupBox">
      <property name="title">
       <string>Cart Items</string>
      </property>
      <layout class="QVBoxLayout" name="cart_item_layout">
       <item>
        <widget class="QListWidget" name="cart_list"/>
       </item>
      </layout>
     </widget>
    </item>

    <!-- GroupBox for Buttons -->
    <item>
     <widget class="QGroupBox" name="buttonsGroupBox">
      <property name="title">
       <string>Actions</string>
      </property>
      <layout class="QHBoxLayout" name="cart_button_layout">
       <item>
        <widget class="QPushButton" name="add_to_cart_button">
         <property name="text">
          <string>Add to Cart</string>
         </property>
        </widget>
       </item>
       <item>
        <widget class="QPushButton" name="remove_from_cart_button">
         <property name="text">
          <string>Remove from Cart</string>
         </property>
        </widget>
       </item>
       <item>
        <widget class="QPushButton" name="update_cart_button">
         <property name="text">
          <string>Update Cart</string>
         </property>
        </widget>
       </item>
      </layout>
     </widget>
    </item>

   </layout>
  </widget>
 </widget>
